
    model{
    
    #Liklihood
    
    for (cell in 1:cells){
    
    #Behavior, add tiny offset
    beh[cell] ~ dbern(rho * X[cell]+0.000001)
    
    #Occurrence
    X[cell] ~ dbern(phi[cell])
    
    #Occ function
    logit(phi[cell]) = alpha + beta * env[cell]  

    }
    
    #Priors
    alpha ~ dnorm(0,0.386)
    beta ~ dnorm(0,0.386)
    
    rho ~ dbeta(1,1)
    
    }
